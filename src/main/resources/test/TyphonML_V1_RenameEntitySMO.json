{
  "version" : "TyphonML_V1",
  "databases" : [
    {
      "type":"relationaldb",
      "name":"MySQL",
      "tables": [
        {
          "name":"userTable",
          "entity":"user"
        }
      ]
    },
    {
      "type":"documentdb",
      "name":"MongoDB",
      "collections" : [
        {
          "name":"ordersCollection",
          "entity":"order"
        }
      ]
    }
  ],
  "entities" : [
    {
      "name":"user",
      "attributes": {
        "name": "string",
        "entrydate": "date"
      }
    },
    {
      "name":"order",
      "attributes":
      {
        "date": "date",
        "totalAmount": "double"
      }
    }
  ]
}
