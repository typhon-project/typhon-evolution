<?xml version="1.0" encoding="ASCII"?>
<typhonml:Model xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:typhonml="http://org.typhon.dsls.typhonml.sirius">
  <entities name="Product">
    <attributes xsi:type="typhonml:Attribute" name="name">
      <type xsi:type="typhonml:StringType" maxSize="256"/>
    </attributes>
    <attributes xsi:type="typhonml:Attribute" name="description">
      <type xsi:type="typhonml:StringType" maxSize="256"/>
    </attributes>
    <attributes xsi:type="typhonml:Attribute" name="price">
      <type xsi:type="typhonml:IntType"/>
    </attributes>
    <attributes xsi:type="typhonml:Attribute" name="productionDate">
      <type xsi:type="typhonml:DateType"/>
    </attributes>
    <attributes xsi:type="typhonml:Attribute" name="availabilityRegion">
      <type xsi:type="typhonml:PolygonType"/>
    </attributes>
    <relations name="reviews" cardinality="zero_many" isContainment="true"/>
    <relations name="tags" type="//@entities.3" cardinality="zero_many"/>
    <relations name="inventory" type="//@entities.2" cardinality="zero_many" isContainment="true"/>
    <relations name="category" type="//@entities.1" cardinality="one"/>
    <relations name="wishes" type="//@entities.11" cardinality="zero_many" opposite="//@entities.11/@relations.1" isContainment="true"/>
    <relations name="concordancesFrom" type="//@entities.10" cardinality="zero_many" opposite="//@entities.10/@relations.0" isContainment="true"/>
    <relations name="concordancesTo" type="//@entities.10" cardinality="zero_many" opposite="//@entities.10/@relations.1" isContainment="true"/>
  </entities>
  <entities name="Category">
    <attributes xsi:type="typhonml:Attribute" name="id">
      <type xsi:type="typhonml:StringType" maxSize="32"/>
    </attributes>
    <attributes xsi:type="typhonml:Attribute" name="name">
      <type xsi:type="typhonml:StringType" maxSize="32"/>
    </attributes>
  </entities>
  <entities name="Item">
    <attributes xsi:type="typhonml:Attribute" name="shelf">
      <type xsi:type="typhonml:IntType"/>
    </attributes>
    <attributes xsi:type="typhonml:Attribute" name="picture">
      <type xsi:type="typhonml:BlobType"/>
    </attributes>
    <relations name="product" type="//@entities.0" cardinality="one" opposite="//@entities.0/@relations.2"/>
  </entities>
  <entities name="Tag">
    <attributes xsi:type="typhonml:Attribute" name="name">
      <type xsi:type="typhonml:StringType" maxSize="64"/>
    </attributes>
    <relations name="synonymsFrom" type="//@entities.4" cardinality="zero_many" opposite="//@entities.4/@relations.0" isContainment="true"/>
    <relations name="synonymsTo" type="//@entities.4" cardinality="zero_many" opposite="//@entities.4/@relations.1" isContainment="true"/>
  </entities>
  <entities name="Synonym">
    <attributes xsi:type="typhonml:Attribute" name="weight">
      <type xsi:type="typhonml:IntType"/>
    </attributes>
    <relations name="source" type="//@entities.3" cardinality="one"/>
    <relations name="target" type="//@entities.3" cardinality="one"/>
  </entities>
  <entities name="Evaluation">
    <attributes xsi:type="typhonml:Attribute" name="body">
      <type xsi:type="typhonml:TextType"/>
    </attributes>
    <relations name="occurrences" type="//@entities.7" cardinality="zero_many" opposite="//@entities.7/@relations.1" isContainment="true"/>
  </entities>
  <entities name="Word">
    <attributes xsi:type="typhonml:Attribute" name="name">
      <type xsi:type="typhonml:StringType" maxSize="256"/>
    </attributes>
    <relations name="occurrences" type="//@entities.7" cardinality="zero_many" opposite="//@entities.7/@relations.0" isContainment="true"/>
  </entities>
  <entities name="Occurrence">
    <attributes xsi:type="typhonml:Attribute" name="times">
      <type xsi:type="typhonml:IntType"/>
    </attributes>
    <relations name="word" type="//@entities.6" cardinality="one"/>
    <relations name="evaluation" type="//@entities.5" cardinality="one"/>
  </entities>
  <entities name="User">
    <attributes xsi:type="typhonml:Attribute" name="name">
      <type xsi:type="typhonml:StringType" maxSize="256"/>
    </attributes>
    <attributes xsi:type="typhonml:Attribute" name="address">
      <type xsi:type="typhonml:StringType" maxSize="256"/>
    </attributes>
    <attributes xsi:type="typhonml:CustomAttribute" name="billing" type="//@customDataTypes.0"/>
    <attributes xsi:type="typhonml:Attribute" name="location">
      <type xsi:type="typhonml:PointType"/>
    </attributes>
    <attributes xsi:type="typhonml:Attribute" name="photoURL">
      <type xsi:type="typhonml:StringType" maxSize="256"/>
    </attributes>
    <attributes xsi:type="typhonml:Attribute" name="avatarURL">
      <type xsi:type="typhonml:StringType" maxSize="256"/>
    </attributes>
    <attributes xsi:type="typhonml:Attribute" name="created">
      <type xsi:type="typhonml:DatetimeType"/>
    </attributes>
    <relations name="biography" type="//@entities.9" opposite="//@entities.9/@relations.0" isContainment="true"/>
    <relations name="reviews" cardinality="zero_many"/>
    <relations name="wishes" type="//@entities.11" cardinality="zero_many" opposite="//@entities.11/@relations.0" isContainment="true"/>
  </entities>
  <entities name="Biography">
    <attributes xsi:type="typhonml:Attribute" name="content">
      <type xsi:type="typhonml:StringType" maxSize="256"/>
    </attributes>
    <relations name="user" type="//@entities.8" cardinality="one" opposite="//@entities.8/@relations.0"/>
  </entities>
  <entities name="Concordance">
    <attributes xsi:type="typhonml:Attribute" name="weight">
      <type xsi:type="typhonml:IntType"/>
    </attributes>
    <relations name="source" type="//@entities.0" cardinality="one"/>
    <relations name="target" type="//@entities.0" cardinality="one"/>
  </entities>
  <entities name="Wish">
    <attributes xsi:type="typhonml:Attribute" name="intensity">
      <type xsi:type="typhonml:IntType"/>
    </attributes>
    <relations name="user" type="//@entities.8" cardinality="one"/>
    <relations name="product" type="//@entities.0" cardinality="one"/>
  </entities>
  <entities name="Review_migrated">
    <attributes xsi:type="typhonml:Attribute" name="location">
      <type xsi:type="typhonml:PointType"/>
    </attributes>
    <attributes xsi:type="typhonml:Attribute" name="screenshot">
      <type xsi:type="typhonml:BlobType"/>
    </attributes>
    <attributes xsi:type="typhonml:Attribute" name="content">
      <type xsi:type="typhonml:TextType"/>
    </attributes>
    <attributes xsi:type="typhonml:Attribute" name="posted">
      <type xsi:type="typhonml:DatetimeType"/>
    </attributes>
    <relations name="product" type="//@entities.0" cardinality="one" isContainment="false"/>
    <relations name="user" type="//@entities.8" cardinality="one" isContainment="false"/>
  </entities>
  <databases xsi:type="typhonml:DocumentDB" name="Reviews">
    <collections name="Biography" entity="//@entities.9"/>
    <collections name="Category" entity="//@entities.1"/>
    <collections name="Evaluation" entity="//@entities.5"/>
  </databases>
  <databases xsi:type="typhonml:RelationalDB" name="Inventory">
    <tables name="UserDB" entity="//@entities.8">
      <indexSpec name="UserNameIndex" attributes="//@entities.8/@attributes.0 //@entities.8/@attributes.3"/>
    </tables>
    <tables name="ProductDB" entity="//@entities.0"/>
    <tables name="TagDB" entity="//@entities.3"/>
    <tables name="ItemDB" entity="//@entities.2"/>
    <tables name="WordDB" entity="//@entities.6"/>
    <tables name="Review_migrated" db="//@databases.1" entity="//@entities.12"/>
  </databases>
  <customDataTypes name="address">
    <elements xsi:type="typhonml:SimpleDataType" name="street">
      <type xsi:type="typhonml:StringType" maxSize="256"/>
    </elements>
    <elements xsi:type="typhonml:SimpleDataType" name="city">
      <type xsi:type="typhonml:StringType" maxSize="256"/>
    </elements>
    <elements xsi:type="typhonml:ComplexDataType" name="zipcode" type="//@customDataTypes.1"/>
    <elements xsi:type="typhonml:SimpleDataType" name="location">
      <type xsi:type="typhonml:PointType"/>
    </elements>
  </customDataTypes>
  <customDataTypes name="zip">
    <elements xsi:type="typhonml:SimpleDataType" name="nums">
      <type xsi:type="typhonml:StringType" maxSize="4"/>
    </elements>
    <elements xsi:type="typhonml:SimpleDataType" name="letters">
      <type xsi:type="typhonml:StringType" maxSize="2"/>
    </elements>
  </customDataTypes>
</typhonml:Model>
