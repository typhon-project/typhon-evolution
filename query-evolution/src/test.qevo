changeOperators[
	rename entity Product as Produit
	split entity Produit { left Produit right Stock}
	remove entity Person
	rename entity Person as Nothing
	merge entities Product Stock as Product
]


// Insert queries here

from Product p select p where p.id == "1230",
from Product p, Stock s select p, s where p.id == "30",
from Person p select p.name,
from Stuff s, Person p select p, s where s.customer == p,

from Product p, Review r, Stock s 
select r, s.quantity 
where p.id == r.reviewed_product, s.prod == p.id
