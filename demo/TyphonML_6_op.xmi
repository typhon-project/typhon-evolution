  <dataTypes xsi:type="typhonml:PrimitiveDataType" name="int"/>
  <dataTypes xsi:type="typhonml:PrimitiveDataType" name="Real"/>
  <dataTypes xsi:type="typhonml:PrimitiveDataType" name="Blob"/>
